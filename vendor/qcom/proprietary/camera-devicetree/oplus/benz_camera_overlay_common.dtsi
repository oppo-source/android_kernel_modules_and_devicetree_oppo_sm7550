		clock-cntl-level = "lowsvs", "svs", "svs_l1", "nominal";
		clock-rates =
			<300000000 0 300000000 0>,
			<400000000 0 300000000 0>,
			<400000000 0 300000000 0>,
			<400000000 0 300000000 0>;
		status = "ok";
	};

	qcom,cam-sync {
		sync-workq-setUX;
	};

	qcom,cam-icp {
